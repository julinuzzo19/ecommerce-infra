              "listen": "prerequest",
              "script": {
                "exec": [
                  "// Función para generar números aleatorios\r",
                  "function getRandomInt(min, max) {\r",
                  "    return Math.floor(Math.random() * (max - min + 1)) + min;\r",
                  "}\r",
                  "\r",
                  "function getRandomFloat(min, max) {\r",
                  "    return (Math.random() * (max - min) + min).toFixed(2);\r",
                  "}\r",
                  "\r",
                  "// Generar todos los valores\r",
                  "pm.environment.set(\"guid1\", pm.variables.replaceIn(\"{{$guid}}\"));\r",
                  "pm.environment.set(\"guid2\", pm.variables.replaceIn(\"{{$guid}}\"));\r",
                  "pm.environment.set(\"guid3\", pm.variables.replaceIn(\"{{$guid}}\"));\r",
                  "pm.environment.set(\"orderNumber\", getRandomInt(1, 10000));\r",
                  "pm.environment.set(\"quantity1\", getRandomInt(1, 20));\r",
                  "pm.environment.set(\"quantity2\", getRandomInt(1, 50));\r",
                  "pm.environment.set(\"price1\", getRandomFloat(10, 500));\r",
                  "pm.environment.set(\"price2\", getRandomFloat(10, 500));"
